/**weui.wxss**/
@import 'weui.wxss';
/**app.wxss**/
.container {
  height: 100%;
  font-size: 14px;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
.comBtn {
  width: 70%;
  color: white;
  background-color: #45C01A;
}
.fixedBtn {
  position: fixed;
  left: 50%;
  bottom: 20px;
  margin-left: -35%;
  width: 70%;
  color: white;
  background-color: #45C01A;
}

.img-right{
  width: 18px;
  height: 18px;
  background-image: url("data:image/png;base64,");
  background-size: 100% 100%;
}
